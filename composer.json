{
    "name": "halfpetal/laravel-identifiable",
    "description": "A simple way to create identifiers for Laravel models.",
    "keywords": ["laravel", "id", "models"],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*",
        "fzaninotto/faker": "~1.4",
        "laravel/laravel": "5.*",
        "codeclimate/php-test-reporter": "^0.3.2",
        "mockery/mockery": "^0.9.5",
        "doctrine/dbal": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Halfpetal\\Laravel\\Identifiable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Halfpetal\\Laravel\\Identifiable\\IdentifiableServiceProdiver"
            ],
            "aliases": {}
        }
    },
    "minimum-stability": "stable"
}
